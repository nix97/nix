  object Memo1: TMemo
    Left = 472
    Height = 160
    Top = 32
    Width = 326
    Lines.Strings = (
      'Polynomial Division'
      ''
      'For example :'
      'x^4-3x^2+4x+5 divided by x^2-x+1'
      ''
      'So :'
      'x^4-3x^2+4x+5 (as Dividend/Polinomial yg dibagi)'
      'x^2-x+1 (as Divisor/pembagi)'
      ''
      'Result :'
      'Quotient(hasil bagi)=x^2+x-3'
      'Remainder(sisa)=8'
      ''
      'because'
      'Dividend = Quotient x Divisor + Remainder  '
      ''
      'How to use :'
      '-Demo --> Click button Sample, Run'
      ''
      '-Input Degree of Dividend (max degree)'
      '-Input Degree of Divisor (max degree)'
      '-Click button Active Grid'
      '-Input Coefficient under variable(just like example)'
      '-Click button Run'
      ''
      'Note :'
      '-coefficient 0 (zero) not counted'
      '-x^0 is 1 (one)'
    )
    ScrollBars = ssVertical
    TabOrder = 5
  end
